<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomNavRoundedCorners" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>



    <style name="fill_round_btn" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/filled_btn_rounded</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/baloo_bhai_2_medium</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="outline_round_btn" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/outline_rounded_btn</item>
        <item name="android:textColor">@color/primary_dark_black_dark_1</item>
        <item name="android:fontFamily">@font/baloo_bhai_2_medium</item>
        <item name="android:textAllCaps">false</item>
    </style>
</resources>